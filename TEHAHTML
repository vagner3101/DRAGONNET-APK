      encrypted = replaceAllreverse(encrypted, "Q", "‚ô®Ô∏è");
      encrypted = replaceAllreverse(encrypted, "q", "üé†");
      encrypted = replaceAllreverse(encrypted, "R", "üïã");
      encrypted = replaceAllreverse(encrypted, "r", "‚õ©Ô∏è");
      encrypted = replaceAllreverse(encrypted, "S", "üìÉ");
